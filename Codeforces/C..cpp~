#define mp(a,b) make_pair(a,b)
#define Q(x) (x) * (x)
#define fi first
#define se second
#define MOD 10009
#define N 1000005

int a[N];

int main(){
    int n , k;
    while( cin >> n >> k ){
        int r = n % k;
        if( r < 3 && r > 0 ){
            puts("-1");
            return 0;
        }
        printf("%d ",k);
        for(int i = 1 ; i <= k-1 ; i++)
            printf(" %d",i);
        
        for(int i = k ; i < n ; i++){
            printf(" %d", (i+1)%k );
        }
    }
    return 0;
}
